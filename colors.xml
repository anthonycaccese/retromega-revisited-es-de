<theme>
   <colorScheme name="colorful-light">
      <variables>
         <!-- Global -->
         <backgroundColor>F3F3F3</backgroundColor>
         <helpBackgroundColor>F3F3F3</helpBackgroundColor>
         <helpIconColor>4d4d4c</helpIconColor>
         <helpTextColor>1D1D1C</helpTextColor>
         <!-- System View -->
         <systemBackgroundColor>${systemColor}</systemBackgroundColor>
         <systemViewNameColor>FFFFFF</systemViewNameColor>
         <systemStatusColor>F3F3F3</systemStatusColor>
         <systemArtworkColor>FFFFFF</systemArtworkColor>
         <systemArtworkSaturation>1</systemArtworkSaturation>
         <systemArtworkBrigtness>0</systemArtworkBrigtness>
         <!-- Gamelist View -->
         <gamelistViewNameColor>1D1D1C</gamelistViewNameColor>
         <gamelistStatusColor>1D1D1C</gamelistStatusColor>
         <gamelistBorderColor>D7D7D7</gamelistBorderColor>
         <gamelistSelectorColor>${systemColor}</gamelistSelectorColor>
         <gamelistPrimaryColor>444444</gamelistPrimaryColor>
         <gamelistSecondaryColor>444444</gamelistSecondaryColor>
         <gamelistSelectedColor>ffffff</gamelistSelectedColor>
      </variables>
   </colorScheme>
